{
    "manifest_version": 3,
    "name": "No More Ad",
    "description": "Skips YouTube Ads.",
    "version": "1.0",
    "icons": { "128": "skip.png" },
    "content_scripts": [
        {
          "matches": ["https://*.youtube.com/*"],
          "js": ["youtube.js"]
        }
      ]
    }

